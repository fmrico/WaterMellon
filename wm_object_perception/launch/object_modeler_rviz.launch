<launch>

	
 <include file="$(find openni2_launch)/launch/openni2.launch">
  	<param name="depth_registration" type="boolean" value="true" />
   	<arg name="publish_tf" value="true" />
  </include>
  
  <node pkg="wm_object_perception" type="wm_modeler" name="wm_modeler" output="screen">
  		<remap from="cloud_in" to="/camera/depth/points" />
  </node>

  <node pkg="rviz" type="rviz" output="screen"
		name="rviz"
		args="-d $(find wm_object_perception)/rviz/wm_modeler.rviz"/>

</launch>
